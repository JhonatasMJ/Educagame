{
  "expo": {
    "name": "Educagame",
    "slug": "educagame",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
  "supportsTablet": true,
  "bundleIdentifier": "com.anonymous.Educagame",
  "infoPlist": {
    "UIBackgroundModes": [
      "remote-notification"
    ]
  }
},
    "android": {
      "permissions": ["NOTIFICATIONS"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff",
        "config": {
          "googleSignIn": {
            "apiKey": "AIzaSyBsF-15bEYrtjYJNNuvaqdhSvnfEzesjcA"
          }
        }
      },
      "package": "com.anonymous.Educagame"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
   "plugins": [
  "expo-router",
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/splash-icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  ],
  [
    "expo-notifications", 
    {
      "icon": "./assets/images/notification-icon.png",
      "color": "#56A6DC",
      "sounds": ["./assets/sound/notification.wav"]
    }
  ],
  "expo-font"
],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "62b7d1f0-0ee4-4778-8709-5027a8b6402d"
      }
    },
    "owner": "vittorpatricio"
  }
}
